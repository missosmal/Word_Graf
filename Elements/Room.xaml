<UserControl x:Class="Word_Graf.Elements.Room"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Word_Graf.Elements"
             mc:Ignorable="d" 
             FontFamily="Bahnscrift left">
    <Grid>
        <Image VerticalAlignment="Top" HorizontalAlignment="Left" Width="30" Height="30" Margin="10"
           Source="/Images/door.png"/>
        <Label x:Name="NameRoom" Margin="45, 0, 10, 0" VerticalAlignment="Top" VerticalContentAlignment="Center"
       Content="Квартира №" Height="50"/>
        <StackPanel x:Name="Parent" Margin="45, 50, 0, -0.4">

        </StackPanel>
    </Grid>
</UserControl>
